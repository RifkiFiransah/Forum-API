{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
        // "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12
        // "ecmaVersion": "latest",
        // "sourceType": "module"
    },
    "rules": {
        "linebreak-style": "off",
        "no-underscore-dangle": "off",
        "max-len": [
            "error",
            {
                "code": 130
            }
        ],
        "class-methods-use-this": "off",
        "no-console": "off"
    }
}